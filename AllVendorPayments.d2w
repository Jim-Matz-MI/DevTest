          @DTW_ASSIGN(orderByDisplay, "Vendor Name (descending), Date Paid (descending), Check Number (descending), Invoice Number")
      %else
          @DTW_ASSIGN(orderBy, "VMVNA1U,PACHKD DESC,PACHK# DESC,PAINV#")
