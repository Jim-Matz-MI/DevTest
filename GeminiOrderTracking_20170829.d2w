  %else
      %if (orderBy == "")
%{
MLH 10 15 2013 changed default sort from order number to end date and end time
 	            @DTW_ASSIGN(orderBy, "GMORD#")
      		    @DTW_ASSIGN(orderByDisplay, "Order Number")


%}
          @DTW_ASSIGN(orderBy, "DC_CYYMMDD DESC, GMENDTM DESC")
