                              <td class="inputnmbr"><input name="srchRecDate" type="text" size="6" maxlength="6">
                                  <a href="javascript:calWindow('srchRecDate');">$(calendarImage)</a></td>
                          </tr>

                          <tr><td class="dsphdr">Ship Via</td>
                              @dtw_assign(operNbr, "operShipVia")
